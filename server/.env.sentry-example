# Sentry Git Integration Configuration
# Add these variables to your .env file for full Git integration

# Sentry Organization (find this in your Sentry dashboard URL)
SENTRY_ORG=your-sentry-org-name

# Sentry Project name (should be "admuse-easy" or your project name)
SENTRY_PROJECT=admuse-easy

# Sentry Auth Token (create at https://sentry.io/settings/auth-tokens/)
# Required scopes: releases
SENTRY_AUTH_TOKEN=your-sentry-auth-token-here

# Git information (automatically set by CI/CD, but can be set manually)
GIT_BRANCH=main
GIT_COMMIT=your-commit-hash

# Release version (automatically detected from package.json if not set)
SENTRY_RELEASE=1.0.0